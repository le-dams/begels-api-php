{
  "name": "begels/begels-api-php",
  "description": "Begels API",
  "type": "library",
  "keywords": [
    "begels"
  ],
  "homepage": "https://github.com/le-dams/begels-api-php",
  "authors": [{
    "name":     "Damien Bourcy",
    "email":    "damien.bourcy@begels.com",
    "homepage": "https://www.begels.com/",
    "role":     "Lead"
  }],
  "license": "BSD-3-Clause",
  "require": {
    "php":            ">=7.1",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
